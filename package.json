{
  "name": "3js-starter",
  "version": "0.1.0",
  "description": "A three.js experiment",
  "main": "src/main.js",
  "repository": {
    "type": "git",
    "url": "http://github.com/rumblesan/soundwall.git"
  },
  "author": "Guy John",
  "license": "BSD-3",
  "scripts": {
    "prebuild": "rm -rf ./dist/*",
    "build": "browserify . > ./dist/app.js",
    "postbuild": "cp -r ./statics/* ./dist/; cp -r ./html/* ./dist/; cat ./style/*.css > ./dist/style.css;",
    "clean": "rm -rf ./dist/*",
    "serve": "http-server ./dist"
  },
  "devDependencies": {
    "browserify": "^9.0.6",
    "http-server": "^0.8.0"
  },
  "dependencies": {
    "teoria": "^1.12.0",
    "thicket": "git://github.com/rumblesan/thicket.git",
    "underscore": "^1.8.3"
  }
}
